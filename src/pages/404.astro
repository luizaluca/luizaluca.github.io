---
import BaseLayout from '../layouts/BaseLayout.astro';
---

<BaseLayout title="Page not found" description="Pagina nu a fost găsită.">
  <div class="container mx-auto px-4 py-20 text-center">
    <h2 class="text-5xl font-bold">404</h2>
    <p class="mt-4">The page you are looking for doesn't exist or has been moved.</p>
    <p class="mt-4"><a class="text-medicenter_color underline" href="/">Back to the Homepage</a></p>
  </div>
</BaseLayout>
